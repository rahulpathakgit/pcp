#ifndef OPENBSD
#define OPENBSD 139
#endif

root {
    hinv
    kernel
    disk
    mem
    network
    filesys
    swap
    pmda
}

hinv {
    ncpu	OPENBSD:0:0
    ndisk	OPENBSD:1:0
    nfilesys	OPENBSD:7:0
    ninterface	OPENBSD:3:17
    physmem	OPENBSD:0:1
    pagesize	OPENBSD:1:3
    cpu
}

hinv.cpu {
    vendor	OPENBSD:0:15
    model	OPENBSD:0:16
}

kernel {
    all
    percpu
    uname
}

